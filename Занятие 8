# -- coding: utf-8 -- 
def code(num) :
	p = [int(d) for d in str(num)]
	for k in p:
		if k!= 0 and num % k!= 0:
			return False
		return True
def prozedura(n):
	num = []
	for i in range(1, n+1):
		if code(i):
			num.append(i)
	return num
n = int(input("Введите число n: "))
result = prozedura(n)
print("Числа, которые делятся на каждую из своих цифр:")
print(result)